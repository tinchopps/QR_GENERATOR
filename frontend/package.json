{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "node -e \"import('fs').then(fs=>{(function walk(d){for(const f of fs.readdirSync(d)){const p=d+'/'+f; if(fs.statSync(p).isDirectory()) walk(p); else if(/\\.js$/.test(p)) fs.unlinkSync(p);} })('src');})\"",
    "build": "npm run clean && vite build",
    "preview": "vite preview",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "axios": "^1.13.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.2",
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.2.1",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/jest": "^29.5.12",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.23",
  "jest": "^29.7.0",
  "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "vite": "^5.2.0"
  }
